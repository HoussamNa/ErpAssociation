{
  "name": "Association-Dashboard-React-Node",
  "version": "1.2.3",
  "description": "React version of an association dashboard",
  "keywords": [
    "react",
    "dashboard",
    "association-dashboard"
  ],
  "author": "Association Dashboard",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile:scss": "sass src/assets/scss/Association-Dashboard-React-Node.scss src/assets/css/Association-Dashboard-React-Node.css",
    "minify:scss": "sass src/assets/scss/Association-Dashboard-React-Node.scss src/assets/css/Association-Dashboard-React-Node.min.css --style compressed",
    "build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.4.0",
    "bootstrap": "4.6.2",
    "chart.js": "2.9.4",
    "classnames": "2.3.2",
    "convert-excel-to-json": "^1.7.0",
    "exceljs": "^4.3.0",
    "express-fileupload": "^1.4.0",
    "moment": "2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "nouislider": "15.4.0",
    "react": ">=16.0.0",
    "react-chartjs-2": "2.11.2",
    "react-copy-to-clipboard": "5.1.0",
    "react-datetime": "3.2.0",
    "react-dom": ">=16.0.0",
    "react-router-dom": "6.11.1",
    "react-scripts": "5.0.1",
    "reactstrap": "^8.10.0",
    "sass": "1.62.1",
    "xlsx": "^0.18.5"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "optionalDependencies": {
    "eslint-plugin-flowtype": "8.0.3",
    "typescript": "5.0.4"
  }
}
